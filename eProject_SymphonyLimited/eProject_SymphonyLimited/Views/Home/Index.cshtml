
@{
    ViewBag.Title = "Index";
}

<div class="page1">
    <div class="cusBanner flex">
        <div class="category width25">
            <div class="cat">
                <span>Course Details</span>
            </div>
            <div class="menu">
                <ul>
                    @{ foreach (var item in ViewBag.categoryLevel2)
                        {
                            <li id="">
                                <a class="defaultMenu" href="">@item.Name</a>
                                <ul>
                                    <div class="subMenu flex" id="subMenu">

                                        @{
                                            foreach (var subItem in ViewBag.categoryLevel3)
                                            {
                                                if (item.EntityId == subItem.ParentId)
                                                {
                                                    <li class="width20" style="display: inline-block">
                                                             <div class="">
                                                                   <a href="" class="subMenuATag">@subItem.Name</a>
                                                             </div>
                                                    </li>
                                            }
                                        }
                                        }
                                    </div>
                                </ul>
                            </li>
                        }
                    }
                 </ul>
                </div>
        </div>
        <div class="mid width50">
            <a href="">
                <div id="carousel-id" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carousel-id" data-slide-to="0" class=""></li>
                        <li data-target="#carousel-id" data-slide-to="1" class=""></li>
                        <li data-target="#carousel-id" data-slide-to="2" class="active"></li>
                    </ol>
                    <div class="carousel-inner" style=" width:100%">
                        <div class="item">
                            <img data-src="holder.js/900x500/auto/#777:#7a7a7a/text:First slide" alt="First slide" src="~/Content/images/bannerCarosel/tedu43.png" width="100%">
                        </div>
                        <div class="item">
                            <img data-src="holder.js/900x500/auto/#666:#6a6a6a/text:Second slide" alt="Second slide" src="~/Content/images/bannerCarosel/gitflow.png" width="100%">
                        </div>
                        <div class="item active">
                            <img data-src="holder.js/900x500/auto/#555:#5a5a5a/text:Third slide" alt="Third slide" src="~/Content/images/bannerCarosel/docker-command.png" width="100%">
                        </div>
                    </div>
                    <a class="left carousel-control border-radius-cus" href="#carousel-id" data-slide="prev">
                        <i class="fa fa-chevron-left" aria-hidden="true"></i>
                    </a>
                    <a class="right carousel-control border-radius-cus" href="#carousel-id" data-slide="next">
                        <i class="fa fa-chevron-right" aria-hidden="true"></i>
                    </a>
                </div>
            </a>
        </div>
        <div class="vCrs width25">
            <div class="table-responsive">
                <table class="table table-bordered table-hover">
                    <tbody>
                        <tr>
                            <td>
                                <div class="img">
                                    <img src="~/Content/images/Course/angular.png" alt="" width="100%">
                                    <h3>Khoá học Android</h3>
                                    <span>6 tháng</span>
                                </div>
                                <div class="price">
                                    <span>$1000</span>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="img">
                                    <img src="~/Content/images/Course/ASP.NET-Core.png" alt="" width="100%">
                                    <h3>Khoá học Android</h3>
                                    <span>6 tháng</span>
                                </div>
                                <div class="price">
                                    <span>$1000</span>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <div class="img">
                                    <img src="~/Content/images/Course/aspnetmvc.jpeg" alt="" width="100%">
                                    <h3>Khoá học Android</h3>
                                    <span>6 tháng</span>
                                </div>
                                <div class="price">
                                    <span>$1000</span>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>

        </div>
    </div>
</div>
<div class="page2">
    <div class="bg">
        <div class="bgEl">
            <h2>Upcoming Entrance Examination</h2>
            <div class="countdown flex">
                <div class="days width25 countdownEl">
                    <p class="big-text" id="days">0</p>
                    <span>days</span>
                </div>
                <div class="hours width25 countdownEl">
                    <p class="big-text" id="hours">0</p>
                    <span>hours</span>
                </div>
                <div class="minutes width25 countdownEl">
                    <p class="big-text" id="mins">0</p>
                    <span>mins</span>
                </div>
                <div class="secs width25 countdownEl">
                    <p class="big-text" id="seconds">0</p>
                    <span>seconds</span>
                </div>
            </div>
            <div class="butt">
                <button>Register</button>
            </div>
        </div>
    </div>
</div>
